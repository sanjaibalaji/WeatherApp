{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { useApolloClient, gql } from '@vue/apollo-composable'; // Ensure this is imported\n\nconst VALIDATE_CITY = gql`\n  query ValidateCity($city: String!) {\n    validateCity(city: $city) {\n      valid\n      message\n    }\n  }\n`;\nexport default {\n  name: 'HomePage',\n  setup() {\n    const city = ref('');\n    const router = useRouter();\n    const popupMessage = ref('');\n    const showPopup = ref(false);\n    const apolloClient = useApolloClient(); // Make sure this is defined\n\n    const navigateToResults = async () => {\n      try {\n        const {\n          data\n        } = await apolloClient.query({\n          query: VALIDATE_CITY,\n          variables: {\n            city: city.value\n          }\n        });\n        if (data.validateCity.valid) {\n          router.push({\n            name: 'WeatherResults',\n            params: {\n              city: city.value\n            }\n          });\n        } else {\n          popupMessage.value = data.validateCity.message;\n          showPopup.value = true;\n        }\n      } catch (error) {\n        console.error(error);\n        popupMessage.value = 'An unexpected error occurred.';\n        showPopup.value = true;\n      }\n    };\n    return {\n      city,\n      navigateToResults,\n      popupMessage,\n      showPopup\n    };\n  }\n};","map":{"version":3,"names":["ref","useRouter","useApolloClient","gql","VALIDATE_CITY","name","setup","city","router","popupMessage","showPopup","apolloClient","navigateToResults","data","query","variables","value","validateCity","valid","push","params","message","error","console"],"sources":["C:\\Users\\Admin\\Documents\\weather\\weather-frontend\\src\\views\\HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"min-h-screen flex flex-col items-center justify-center bg-gray-100\">\r\n    <h1 class=\"text-2xl mb-4\">Weather 