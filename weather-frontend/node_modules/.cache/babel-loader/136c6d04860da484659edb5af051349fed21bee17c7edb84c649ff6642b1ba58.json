  }\r\n    });\r\n\r\n    const bgClass = computed(() => {\r\n      if (!weather.value) return '';\r\n      const temp = weather.value.temperature;\r\n\r\n      if (temp < 0) return 'bg-blue-900';\r\n      if (temp < 10) return 'bg-blue-500';\r\n      if (temp < 20) return 'bg-green-500';\r\n      if (temp < 30) return 'bg-yellow-500';\r\n      return 'bg-red-500';\r\n    });\r\n\r\n    const cardClass = computed(() => 'bg-white bg-opacity-60 backdrop-blur-sm');\r\n    const textClass = computed(() => 'text-gray-900');\r\n\r\n    return {\r\n      weather,\r\n      hourlyForecast,\r\n      city: props.city,\r\n      loading,\r\n      error,\r\n      bgClass,\r\n      cardClass,\r\n      textClass,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Add any scoped styles here */\r\n</style>\r\n"],"mappings":";;EAgJSA,KAAK,EAAC;AAA2C;mBAhJ1D;;;uBA+IEC,mBAAA,CAcM;IAdAD,KAAK,EA/IbE,eAAA,EA+IeC,MAAA,CAAAC,OAAO,EAAQ,kFAAkF;MAC5GC,mBAAA,CAYM,OAZNC,UAYM,GAXJD,mBAAA,CAAgF;IAA3EL,KAAK,EAjJhBE,eAAA,EAiJkBC,MAAA,CAAAI,SAAS,EAAQ,eAAe;KAAC,sBAAoB,GAAAC,gBAAA,CAAGL,MAAA,CAAAM,IAAI,yBAC7DN,MAAA,CAAAO,OAAO,I,cAAlBT,mBAAA,CAQM;IA1JZU,GAAA;IAkJ2BX,KAAK,EAlJhCE,eAAA,EAkJkCC,MAAA,CAAAS,SAAS,EAAQ,4BAA4B;MACvEP,mBAAA,CAA+F;IAAzFL,KAAK,EAnJnBE,eAAA,EAmJqBC,MAAA,CAAAI,SAAS,EAAQ,oBAAoB;sBAAIJ,MAAA,CAAAO,OAAO,CAACG,WAAW,CAACC,OAAO,OAAM,IAAE,wBACzFT,mBAAA,CAAuE;IAAjEL,KAAK,EApJnBE,eAAA,EAoJqBC,MAAA,CAAAI,SAAS,EAAQ,SAAS;sBAAIJ,MAAA,CAAAO,OAAO,CAACK,WAAW,yBAC9DV,mBAAA,CAAqH;IAA/GW,GAAG,sCAAsCb,MAAA,CAAAO,OAAO,CAACO,IAAI;IAAWC,GAAG,EAAC,cAAc;IAAClB,KAAK,EAAC;0BArJvGmB,UAAA,GAsJQd,mBAAA,CAAwD;IAAlDL,KAAK,EAtJnBE,eAAA,EAsJqBC,MAAA,CAAAI,SAAS,EAAQ,SAAS;sBAAIJ,MAAA,CAAAM,IAAI,yBAC/CJ,mBAAA,CAAmF;IAA7EL,KAAK,EAvJnBE,eAAA,EAuJqBC,MAAA,CAAAI,SAAS,EAAQ,SAAS;0BAAQa,IAAI,GAAGC,kBAAkB,2BACxEhB,mBAAA,CAA8F;IAAxFL,KAAK,EAxJnBE,eAAA,EAwJqBC,MAAA,CAAAI,SAAS,EAAQ,SAAS;KAAC,aAAW,GAAAC,gBAAA,CAAGL,MAAA,CAAAO,OAAO,CAACY,UAAU,CAACR,OAAO,OAAM,IAAE,wBACxFT,mBAAA,CAA+G;IAAzGL,KAAK,EAzJnBE,eAAA,EAyJqBC,MAAA